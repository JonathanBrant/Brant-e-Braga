[1mdiff --git a/script/script.js b/script/script.js[m
[1mnew file mode 100644[m
[1mindex 0000000..000198f[m
[1m--- /dev/null[m
[1m+++ b/script/script.js[m
[36m@@ -0,0 +1,27 @@[m
[32m+[m[32mvar slideIndex = 1;[m
[32m+[m[32mshowSlides(slideIndex);[m
[32m+[m
[32m+[m[32mfunction plusSlides(n) {[m
[32m+[m[32m  showSlides(slideIndex += n);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction currentSlide(n) {[m
[32m+[m[32m  showSlides(slideIndex = n);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction showSlides(n) {[m
[32m+[m[32m  var i;[m
[32m+[m[32m  var slides = document.getElementsByClassName("mySlides");[m
[32m+[m[32m  var dots = document.getElementsByClassName("dot");[m
[32m+[m[32m  if (n > slides.length) {slideIndex = 1}[m
[32m+[m[32m  if (n < 1) {slideIndex = slides.length}[m
[32m+[m[32m  for (i = 0; i < slides.length; i++) {[m
[32m+[m[32m      slides[i].style.display = "none";[m
[32m+[m[32m  }[m
[32m+[m[32m  for (i = 0; i < dots.length; i++) {[m
[32m+[m[32m      dots[i].className = dots[i].className.replace(" active", "");[m
[32m+[m[32m  }[m
[32m+[m[32m  slides[slideIndex-1].style.display = "block";[m
[32m+[m[32m  dots[slideIndex-1].className += " active";[m
[32m+[m[32m}[m
[32m+[m
